    "futures": {
      "basePath": "/api/futures",
      "description": "Futures trading operations including positions, orders, and margin management",
      "routes": [
        {
          "method": "GET",
          "path": "/positions",
          "description": "List all open futures positions",
          "response": { "type": "array", "items": "Position" }
        },
        {
          "method": "POST",
          "path": "/order",
          "description": "Place a new futures order",
          "requestBody": {
            "symbol": "string (required)",
            "side": "'BUY' | 'SELL' (required)",
            "type": "'MARKET' | 'LIMIT' (required)",
            "qty": "number (required)",
            "price": "number (optional, required for LIMIT)",
            "leverage": "number (optional)",
            "stopLoss": "number (optional)",
            "takeProfit": "number (optional)",
            "reduceOnly": "boolean (optional)",
            "marginMode": "'ISOLATED' | 'CROSS' (optional)"
          }
        },
        {
          "method": "DELETE",
          "path": "/position/:id",
          "description": "Close an existing futures position",
          "params": { "id": "string (position ID)" }
        },
        {
          "method": "GET",
          "path": "/margin",
          "description": "Get margin account information"
        },
        {
          "method": "GET",
          "path": "/orders",
          "description": "Get all orders (open and recent)"
        },
        {
          "method": "DELETE",
          "path": "/order/:id",
          "description": "Cancel a pending order",
          "params": { "id": "string (order ID)" }
        },
        {
          "method": "POST",
          "path": "/leverage",
          "description": "Set leverage for a symbol",
          "requestBody": {
            "symbol": "string (required)",
            "leverage": "number (required, 1-125)"
          }
        }
      ]
    },
