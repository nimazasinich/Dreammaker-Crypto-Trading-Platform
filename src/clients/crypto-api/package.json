{
  "name": "@dreammaker/crypto-api-client",
  "version": "1.0.0",
  "description": "A comprehensive TypeScript/JavaScript client for cryptocurrency data API hosted on HuggingFace Space",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node examples/complete-example.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cryptocurrency",
    "crypto",
    "bitcoin",
    "ethereum",
    "trading",
    "api-client",
    "typescript",
    "huggingface",
    "market-data",
    "sentiment-analysis",
    "ai-trading",
    "crypto-api"
  ],
  "author": "DreamMaker Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/crypto-api-client.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/crypto-api-client/issues"
  },
  "homepage": "https://github.com/yourusername/crypto-api-client#readme",
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
